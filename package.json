  "scripts": {
    "dev": "next dev",
    "dev:fast": "next dev --turbo",
    "dev:no-lint": "SKIP_ENV_VALIDATION=true next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fast": "next lint --cache --cache-location .next/cache/.eslintcache",
    "lint:fix": "next lint --fix --cache",
    "lint:staged": "eslint --fix",
    "type-check": "tsc --noEmit --incremental --tsBuildInfoFile .next/cache/tsbuildinfo",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:staged": "prettier --write",
    "check": "prettier --check \"**/*.{ts,tsx,js,jsx,md,mdx,css,yaml,yml}\"",
    "build:icons": "tsx src/assets/iconify-icons/bundle-icons-css.ts",
    "postinstall": "npm run build:icons",
    "prepare": "husky",
    "clean": "rimraf .next out dist",
    "clean:cache": "rimraf .next/cache node_modules/.cache"
  },
